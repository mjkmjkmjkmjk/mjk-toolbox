curl -skSL \
    -X POST \
    -d @<JSON_FILE> \
    -H "Authorization: token=$(dcos config show core.dcos_acs_token)" \
    -H "Content-Type: application/json" \
    $(dcos config show core.dcos_url)/service/chronos/scheduler/iso8601

curl -skSL \
    -X PUT \
    -H "Authorization: token=$(dcos config show core.dcos_acs_token)" \
    -H "Content-Type: application/json" \
    $(dcos config show core.dcos_url)/service/chronos/scheduler/job/<job-name>

curl -skSL \
    -X DELETE \
    -H "Authorization: token=$(dcos config show core.dcos_acs_token)" \
    -H "Content-Type: application/json" \
    $(dcos config show core.dcos_url)/service/chronos/scheduler/job/<job-name>

curl -skSL \
    -X GET \
    -H "Authorization: token=$(dcos config show core.dcos_acs_token)" \
    -H "Content-Type: application/json" \
    $(dcos config show core.dcos_url)/service/chronos/scheduler/jobs | \
    jq -er .[].name
