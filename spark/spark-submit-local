dcos spark run --submit-args='-Dspark.mesos.coarse=true -Dspark.mesos.executor.docker.image=mesosphere/spark:1.0.2-2.0.0 -Dspark.mesos.uris=http://hdfs.marathon.mesos:9000/v1/connect/hdfs-site.xml,http://hdfs.marathon.mesos:9000/v1/connect/core-site.xml -Dspark.cores.max=4 --driver-cores 1 --driver-memory 1g -Dspark.executor.cores=2 -Dspark.executor.memory=1g -Dspark.eventLog.enabled=true -Dspark.eventLog.dir=hdfs://hdfs/history -Dspark.eventLog.compress=false --class org.apache.spark.examples.SparkPi https://downloads.mesosphere.com/spark/assets/spark-examples_2.10-1.4.0-SNAPSHOT.jar 30'

dcos spark run --submit-args='-Dspark.mesos.coarse=true -Dspark.mesos.executor.docker.image=mesosphere/spark:1.0.2-2.0.0 -Dspark.mesos.uris=http://hdfs.marathon.mesos:9000/v1/connect/hdfs-site.xml,http://hdfs.marathon.mesos:9000/v1/connect/core-site.xml -Dspark.cores.max=4 --driver-cores 1 --driver-memory 1g -Dspark.executor.cores=2 -Dspark.executor.memory=1g -Dspark.eventLog.enabled=true -Dspark.eventLog.dir=hdfs://hdfs/history -Dspark.eventLog.compress=false --class org.apache.spark.examples.HdfsTest https://downloads.mesosphere.com/spark/assets/spark-examples_2.10-1.4.0-SNAPSHOT.jar hdfs://hdfs/history/d4f88965-b110-4d59-a178-dada4ab2f749-0003-driver-20160917185601-0006'

